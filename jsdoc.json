{
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_models/|docs)"
    },
    "plugins": ["plugins/markedown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template": "./custom-doc",
        "tutorials": "./tutorials",
        "readme": "./README.md"
    }
}